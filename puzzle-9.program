function drawRight(x) {
  while (x>=0) {
		setColor ("red");
  	right();
    x=x-1}
	}

function drawDown (x) {
  while (x>=0) {
    setColor ("red");
    down();
    x=x-1}
}

function drawLeft (x) {
  while (x>=0) {
    setColor ("red");
    left();
    x=x-1}
}

function drawUp (x) {
  while (x>=0) {
    setColor ("red");
    up();
    x=x-1}
}

drawRight(70);
drawDown(50);
drawLeft(70);
drawUp(50);
